<?xml version="1.1" encoding="UTF-8" standalone="no"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">

<!--    <include file="classpath:/liquibase/v7/changelog-master-v7.xml"/>-->
<!--    <include file="classpath:/liquibase/v8/changelog-master-v8.xml"/>-->
    
    <changeSet id="1" author="tiagobarbosa02@outlook.com">
        <preConditions>
            <not>
                <tableExists tableName="person"/>
            </not>
        </preConditions>
        <createTable tableName="person">
            <column name="id" type="bigint" autoIncrement="true">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="name" type="varchar(2000)"/>
            <column name="email" type="varchar(50)"/>
        </createTable>
    </changeSet>
</databaseChangeLog>
